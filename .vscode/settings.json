{
  "cSpell.words": [
    "CAGR",
    "dotenv",
    "IAGG",
    "mypy",
    "pmatrix",
    "psycopg2",
    "pyarrow",
    "pydocstyle",
    "pylint",
    "Streamlit",
    "unpivot",
    "VARCHAR",
    "venv"
  ],
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
  }
}
